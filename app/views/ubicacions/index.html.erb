<h1>Listing ubicacions</h1>

<table>
  <tr>
    <th>Descripcion</th>
  </tr>

<% for ubicacion in @ubicacions %>
  <tr>
    <td><%=h ubicacion.descripcion %></td>
    <td><%= link_to 'Show', ubicacion %></td>
    <td><%= link_to 'Edit', edit_ubicacion_path(ubicacion) %></td>
    <td><%= link_to 'Destroy', ubicacion, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New ubicacion', new_ubicacion_path %>
